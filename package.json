{
  "name": "nest7-boilerplate",
  "version": "0.0.1",
  "description": "NestJS v7 boilerplate",
  "author": "Samuel MARLHENS",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prettier:check": "prettier --check ./**/*.{ts,js,json,*rc}",
    "prettier:write": "prettier --write ./**/*.{ts,js,json,*rc}",
    "precommit": "lint-staged && npm run lint && npm run test",
    "prepush": "npm run test",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nestjs/common": "8.0.4",
    "@nestjs/config": "0.6.3",
    "@nestjs/core": "8.0.4",
    "@nestjs/platform-express": "8.0.4",
    "@nestjs/swagger": "4.8.2",
    "@nestjs/typeorm": "7.1.5",
    "pg": "8.6.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.6.7",
    "swagger-ui-express": "4.1.6",
    "typeorm": "0.2.34"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@nestjs/cli": "8.0.2",
    "@nestjs/schematics": "7.3.1",
    "@nestjs/testing": "8.0.4",
    "@types/express": "4.17.13",
    "@types/jest": "26.0.24",
    "@types/node": "14.17.5",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "4.28.3",
    "@typescript-eslint/parser": "4.28.3",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.30.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.4",
    "husky": "6.0.0",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "prettier": "2.3.2",
    "supertest": "6.1.3",
    "ts-jest": "26.5.6",
    "ts-loader": "8.3.0",
    "ts-node": "9.1.1",
    "tsconfig-paths": "3.10.1",
    "typescript": "4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
